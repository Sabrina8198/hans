1.ADD
def add(param1, param2):
    return(param1+param2)
param1 = 1
param2 = 2


add(param1, param2)

2.centuryFromYear
def centuryFromYear(year):
    return int((year-1)/100) + 1

3.checkPalindrome
def checkPalindrome(inputString):
    def reverse(inputString):
        return inputString[::-1]
    if inputString == reverse(inputString):
        return True
    else:
        return False

4.adjacentElementsProduct
def adjacentElementsProduct(inputArray):
    abc = [inputArray[i]*inputArray[i+1]
           for i in range(0, len(inputArray)-1)]
    return max(abc)
    
5.shapeArea
def shapeArea(n):
    sum = 1
    for i in range(1,n):
        sum += 4*i
    return sum
    
6.makeArrayConsecutive2
def makeArrayConsecutive2(statues):
    statues = sorted(statues, reverse = False)
    abc = [statues[i+1]-statues[i]-1 for i in range(0, len(statues)-1)]
    return sum(abc)
